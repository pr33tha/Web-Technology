import java.util.ArrayList;

public class DataDisplay {
    public static void main(String[] args) {
        // Example data
        ArrayList<String> data = new ArrayList<>();
        data.add("Coffee");
        data.add("Juice");
        data.add("Sandwich");
        data.add("Pizza");

        // Display the data
        System.out.println("My Data:");
        for (String item : data) {
            System.out.println("- " + item);
        }
    }
}
